    <script>

        $(document).ready(function () {

            $("#headerImg").addClass("imageRot")

            setTimeout(function () {
                $("#headerImg").removeClass("imageRot")
            }, 22200)

            $('#headerImg').click(function () {
                $("#headerImg").addClass("imageRot")
                setTimeout(function () {
                    $("#headerImg").removeClass("imageRot")
                }, 22200)
            })

            // method selection
            $('#method1').click(function (e) {
                e.preventDefault()
                $('.signupMethod').hide()
                $('.methodSelector').removeClass('active')
                $('#method1').addClass('active')
                $('#signupMethod1').show()
            })

            $('#method2').click(function (e) {
                e.preventDefault()
                $('.signupMethod').hide()
                $('.methodSelector').removeClass('active')
                $('#method2').addClass('active')
                $('#signupMethod2').show()
            })

            $('#method3').click(function (e) {
                e.preventDefault()
                $('.signupMethod').hide()
                $('.methodSelector').removeClass('active')
                $('#method3').addClass('active')
                $('#signupMethod3').show()
            })
        })
    </script>
